{
    "name": "beatlabs/pyr",
    "description": "A Prometheus bridge for Lumen and Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Beat"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "^5.3",
        "illuminate/routing": "^5.3",
        "Jimdo/prometheus_client_php": "dev-master"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Jimdo/prometheus_client_php.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Beat\\Pyr\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^0.9.5",
        "friendsofphp/php-cs-fixer": "2.*",
        "orchestra/testbench": "^3.5"
    }
}
